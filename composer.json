{
  "name": "schrapel/cmb2-loader",
  "type": "wordpress-plugin",
  "license": "MIT",
  "description": "Autoload your CMB2 metaboxes and fields using JSON, YAML or PHP files",
  "homepage": "https://github.com/schrapel/cmb2-loader",
  "authors": [
    {
      "name": "Toby Schrapel",
      "email": "toby@tobyschrapel.com",
      "homepage": "https://github.com/schrapel"
    }
  ],
  "keywords": [
    "wordpress"
  ],
  "support": {
    "issues": "https://github.com/schrapel/cmb2-loader/issues"
  },
  "require": {
    "php": ">=7.0",
    "composer/installers": "~1.0",
    "hassankhan/config": "^0.10.0",
    "symfony/yaml": "^3.2"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "2.*"
  },
  "autoload": {
    "psr-4": {
      "Schrapel\\CMB2Loader\\": "src/"
    }
  },
  "scripts": {
    "test": [
      "vendor/bin/phpcs --extensions=php --ignore=vendor/ ."
    ]
  }
}